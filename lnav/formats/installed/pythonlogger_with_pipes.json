{
    "pythonlogger_pipes": {
        "title": "Python logger format with pipes",
        "description": "Log format used by python logger class with pipes",
        "url": "",
        "regex": {
            "main" : {
                "pattern": "^(?<timestamp>\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}.\\d{3})\\s+\\|\\s+(?<level>\\w+)\\s+\\|\\s+(?<body>.*)$"
            }
        },
        "level-field": "level",
        "level": {
            "critical": "CRITICAL",
            "error": "ERROR",
            "warning": "WARNING",
            "info": "INFO",
            "debug": "DEBUG"
        },
        "value": {
            "level": { "kind" : "string", "identifier" : true }
        },
        "sample": [
            {
                "line": "2021-10-12 23:37:00.283 | INFO     | analyzers.base.scanner_base:_get_scanners:226 - imported all scanners"
            }
        ]
    }
}
