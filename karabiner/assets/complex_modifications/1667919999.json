{
    "title": "My Complex mods",
    "rules": [
        {
            "description": "Hyper + e = del",
            "manipulators": [
                {
                    "from": {
                        "key_code": "e",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "left_command",
                                "left_control",
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "delete_forward"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Hyper + 1 = menu bar highlight",
            "manipulators": [
                {
                    "from": {
                        "key_code": "1",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "left_command",
                                "left_control",
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "f2",
                            "modifiers": [
                              "left_control"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Hyper + 2 = dock highlight",
            "manipulators": [
                {
                    "from": {
                        "key_code": "2",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "left_command",
                                "left_control",
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "f3",
                            "modifiers": [
                              "left_control"
                            ]
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Hyper + TAB = return",
            "manipulators": [
                {
                    "from": {
                        "key_code": "tab",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "left_command",
                                "left_control",
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "return_or_enter"
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "Hyper + ` = backspace",
            "manipulators": [
                {
                    "from": {
                        "key_code": "grave_accent_and_tilde",
                        "modifiers": {
                            "mandatory": [
                                "left_shift",
                                "left_command",
                                "left_control",
                                "left_option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "delete_or_backspace"
                        }
                    ],
                    "type": "basic"
                }
            ]
        }
    ]
}