{
  "title": "Hyper Key sublayer \"x\" - Xray",
  "rules": [
    {
      "description": "Hyper Key sublayer \"x\" - Xray",
      "manipulators": [
        {
          "description": "Toggle Hyper sublayer x",
          "type": "basic",
          "from": {
            "key_code": "x",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_control",
                "left_shift",
                "left_option"
              ]
            }
          },
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "hyper_sublayer_x",
                "value": 0
              }
            }
          ],
          "to": [
            {
              "set_variable": {
                "name": "hyper_sublayer_x",
                "value": 1
              }
            }
          ],
          "conditions": [
            {
              "type": "variable_if",
              "name": "hyper_sublayer_o",
              "value": 0
            },
            {
              "type": "variable_if",
              "name": "hyper_sublayer_w",
              "value": 0
            }
          ]
        },
        {
          "to": [
            {
              "shell_command": "open 'http://localhost:8083/ui/scans-list/repositories'"
            }
          ],
          "description": "Open Scans List",
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {
              "mandatory": [
                "any"
              ]
            }
          },
          "conditions": [
            {
              "type": "variable_if",
              "name": "hyper_sublayer_x",
              "value": 1
            }
          ]
        },
        {
          "to": [
            {
              "shell_command": "open 'http://localhost:8083/ui/repos/tree/General/'"
            }
          ],
          "description": "Open Artifactory Tree",
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": [
                "any"
              ]
            }
          },
          "conditions": [
            {
              "type": "variable_if",
              "name": "hyper_sublayer_x",
              "value": 1
            }
          ]
        },
        {
          "to": [
            {
              "shell_command": "open 'http://localhost:8083/ui/admin/xray/policiesGovernance/policies'"
            }
          ],
          "description": "Open Watches and Policies",
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": [
                "any"
              ]
            }
          },
          "conditions": [
            {
              "type": "variable_if",
              "name": "hyper_sublayer_x",
              "value": 1
            }
          ]
        },
        {
          "to": [
            {
              "shell_command": "open 'http://localhost:8083/ui/admin/xray/general/indexed_resources'"
            }
          ],
          "description": "Open Indexed Resources",
          "type": "basic",
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": [
                "any"
              ]
            }
          },
          "conditions": [
            {
              "type": "variable_if",
              "name": "hyper_sublayer_x",
              "value": 1
            }
          ]
        }
      ]
    }
  ]
}