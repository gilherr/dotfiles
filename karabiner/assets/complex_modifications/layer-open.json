{
    "title": "Hyper Key sublayer \"o\" - Open Apps",
    "rules": [
      {
        "description": "Hyper Key sublayer \"o\" - Open Apps",
        "manipulators": [
          {
            "description": "Toggle Hyper sublayer o",
            "type": "basic",
            "from": {
              "key_code": "o",
              "modifiers": {
                "mandatory": [
                  "left_command",
                  "left_control",
                  "left_shift",
                  "left_option"
                ]
              }
            },
            "to_after_key_up": [
              {
                "set_variable": {
                  "name": "hyper_sublayer_o",
                  "value": 0
                }
              }
            ],
            "to": [
              {
                "set_variable": {
                  "name": "hyper_sublayer_o",
                  "value": 1
                }
              }
            ],
            "conditions": [
              {
                "type": "variable_if",
                "name": "hyper_sublayer_x",
                "value": 0
              },
              {
                "type": "variable_if",
                "name": "hyper_sublayer_w",
                "value": 0
              }
            ]
          },
          

          {
            "description": "Alternative to 'o' by using `1`",
            "type": "basic",
            "from": {
              "key_code": "1",
              "modifiers": {
                "mandatory": [
                  "left_command",
                  "left_control",
                  "left_shift",
                  "left_option"
                ]
              }
            },
            "to_after_key_up": [
              {
                "set_variable": {
                  "name": "hyper_sublayer_o",
                  "value": 0
                }
              }
            ],
            "to": [
              {
                "set_variable": {
                  "name": "hyper_sublayer_o",
                  "value": 1
                }
              }
            ],
            "conditions": [
              {
                "type": "variable_if",
                "name": "hyper_sublayer_w",
                "value": 0
              },
              {
                "type": "variable_if",
                "name": "hyper_sublayer_s",
                "value": 0
              },
              {
                "type": "variable_if",
                "name": "hyper_sublayer_v",
                "value": 0
              },
              {
                "type": "variable_if",
                "name": "hyper_sublayer_c",
                "value": 0
              },
              {
                "type": "variable_if",
                "name": "hyper_sublayer_r",
                "value": 0
              }
            ]
          },

          
          {
            "to": [
              {
                "shell_command": "open -a 'Google Chrome.app'"
              }
            ],
            "description": "Open -a 'Google Chrome.app'",
            "type": "basic",
            "from": {
              "key_code": "c",
              "modifiers": {
                "mandatory": [
                  "any"
                ]
              }
            },
            "conditions": [
              {
                "type": "variable_if",
                "name": "hyper_sublayer_o",
                "value": 1
              }
            ]
          },
